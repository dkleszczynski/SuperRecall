<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SuperRecall.Resources">

    <ContextMenu x:Key="QueueMenu">
        <MenuItem FontSize="18" Header="{x:Static local:ElementsView.PlanRevise}"></MenuItem>
        <Separator></Separator>
        <MenuItem FontSize="18" Header="{x:Static local:ElementsView.DeleteElement}"></MenuItem>
    </ContextMenu>
    <ContextMenu x:Key="ReviseMenu">
        <MenuItem FontSize="18" Header="{x:Static local:ElementsView.LearnAgain}"></MenuItem>
        <Separator></Separator>
        <MenuItem FontSize="18" Header="{x:Static local:ElementsView.DeleteElement}"></MenuItem>
    </ContextMenu>

</ResourceDictionary>